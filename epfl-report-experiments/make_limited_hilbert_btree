time ~/phd/bbpdias/BBPDIAS/Test --input ~/phd/data/bbp/testcircuit/BlueConfig --output mesh_lhilbert_64_6m --data mesh --limit 6000000 --index limited-b-hilbert --hilbertvalues mesh_l64_6m.hilbert.data --noqueries --keysize 64
time ~/phd/bbpdias/BBPDIAS/Test --input ~/phd/data/bbp/testcircuit/BlueConfig --output mesh_lhilbert_64_7m --data mesh --limit 7000000 --index limited-b-hilbert --hilbertvalues mesh_l64_7m.hilbert.data --noqueries --keysize 64
