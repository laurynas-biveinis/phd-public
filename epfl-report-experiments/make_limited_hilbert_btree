time ~/phd/bbpdias/BBPDIAS/Test --input ~/phd/data/bbp/testcircuit/BlueConfig --output mesh_lhilbert_64_9m --data mesh --limit 9000000 --index limited-b-hilbert --hilbertvalues mesh_l64_9m.hilbert.data --noqueries --keysize 64
time ~/phd/bbpdias/BBPDIAS/Test --input ~/phd/data/bbp/testcircuit/BlueConfig --output mesh_lhilbert_64_10m --data mesh --limit 10000000 --index limited-b-hilbert --hilbertvalues mesh_l64_10m.hilbert.data --noqueries --keysize 64

# 2nd pass: no Hilbert values, no checking, no queries, index build performance
time ~/phd/bbpdias/BBPDIAS/Test --input ~/phd/data/bbp/testcircuit/BlueConfig --output mesh_lhilbert_64_1m --data mesh --limit 1000000 --index limited-b-hilbert --nocheck --noqueries --keysize 64
